{
  "devDependencies": {
    "eleventy": "^1.0.6",
    "node-sass": "^8.0.0",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "@11ty/eleventy": "^2.0.1"
  },
  "name": "paragon-site",
  "version": "1.0.0",
  "description": "website for paragon studios",
  "main": "index.js",
  "scripts": {
    "build": "npm-run-all build:css build:html --silent",
    "build:css": "node-sass src/site/include/sass/style.sass > src/site/css/style.css",
    "build:html": "eleventy",
    "watch": "nodemon -w src --exec \"npm run build -s\" --ext \"md,sass\"",
    "start": "npm run watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Paragon-Studios/Paragon-Site.git"
  },
  "keywords": [
    "site",
    "web",
    "website",
    "sass"
  ],
  "author": "R-unic, Paragon-Studios",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Paragon-Studios/Paragon-Site/issues"
  },
  "homepage": "https://github.com/Paragon-Studios/Paragon-Site#readme"
}
